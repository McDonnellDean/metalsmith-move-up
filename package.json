{
  "name": "metalsmith-move-up",
  "version": "3.0.0",
  "description": "a MetalSmith plugin for moving the full contents of a directory up one or more levels",
  "main": "lib/plugin.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "lint": "standard",
    "test": "tape test/*.js"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "files": [
    "eg/*",
    "lib/*",
    "CONTRIBUTING.md"
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/rbardini/metalsmith-move-up.git"
  },
  "keywords": [
    "metalsmith",
    "directory",
    "plugin"
  ],
  "author": "Rafael Bardini <rafael@rbardini.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rbardini/metalsmith-move-up/issues"
  },
  "homepage": "https://github.com/rbardini/metalsmith-move-up",
  "devDependencies": {
    "pre-commit": "1.2.2",
    "standard": "16.0.3",
    "tape": "5.2.2"
  },
  "dependencies": {
    "async": "^3.2.0",
    "multimatch": "^5.0.0"
  }
}
